{
  "name": "reading",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "codemirror": "^5.47.0",
    "coveralls": "^3.0.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest-enzyme": "^7.0.2",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-codemirror2": "^6.0.0",
    "react-dom": "^16.8.6",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-test-render": "^1.1.1",
    "sg2ts": "^1.0.1",
    "source-map-explorer": "^2.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:develop": "react-scripts test --coverage",
    "test:all": "react-scripts test --watchAll",
    "eject": "react-scripts eject",
    "lint": "eslint --fix --ignore-pattern '/src/serviceWorker.js'",
    "build:view": "npm run build && npx serve -s build",
    "coverage:ci": "react-scripts test --coverage --coverageReporters=text-lcov | coveralls"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test:develop"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/component/*.{js,jsx,ts,tsx}",
      "src/page/*.{js,jsx,ts,tsx}",
      "src/util/*.{js,jsx,ts,tsx}",
      "src/*.{js, jsx, ts, tsx}",
      "!src/serviceWorker.js",
      "!src/index.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2"
  },
  "homepage": "https://limengke123.github.io/tiny-waffle"
}
